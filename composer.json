{
    "name": "pacoorozco/ssham",
    "description": "SSH Access Manager Web Interface.",
    "keywords": [
        "ssh-key",
        "ssh",
        "ssh key manager",
        "keyring",
        "access management",
        "laravel"
    ],
    "homepage": "https://github.com/pacoorozco/ssham",
    "license": "GPL-3.0-or-later",
    "type": "project",
    "authors": [
        {
            "name": "Paco Orozco",
            "email": "paco@pacorozco.info",
            "homepage": "https://pacoorozco.info",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/pacoorozco/ssham/issues"
    },
    "require": {
        "php": "^8.0.2",
        "almasaeed2010/adminlte": "~3.2",
        "bensampo/laravel-enum": "^6.0.0",
        "fruitcake/laravel-cors": "^3.0.0",
        "guilhermegonzaga/presenter": "^1.0.6",
        "larapacks/setting": "^3.0.1",
        "laravel-json-api/laravel": "^2.4.0",
        "laravel/framework": "^9.35.1",
        "laravel/sanctum": "^v2.15.1",
        "laravel/ui": "^3.4.6",
        "laravelcollective/html": "^6.3.0",
        "pacoorozco/openssh": "^0.4.0",
        "phpseclib/phpseclib": "^3.0.16",
        "spatie/laravel-activitylog": "^4.6.0",
        "spatie/laravel-permission": "^5.5.5",
        "spatie/laravel-searchable": "^1.11.0",
        "yajra/laravel-datatables-oracle": "^10.1.6"
    },
    "require-dev": {
        "brianium/paratest": "^6.6",
        "doctrine/dbal": "^3.4.5",
        "fakerphp/faker": "^1.20.0",
        "laravel-json-api/testing": "^1.1.1",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.3.1",
        "nunomaduro/larastan": "^2.2.1",
        "phpunit/phpunit": "^9.5.25",
        "spatie/laravel-ignition": "^1.5.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "test": [
            "@php artisan test --parallel"
        ],
        "test-coverage": [
            "@php artisan test --parallel --coverage-html test-coverage"
        ],
        "build": [
            "@composer archive --format=zip --dir=dist"
        ]
    },
    "archive": {
        "exclude": [
            "/utils",
            "/.*",
            "/docs",
            "!/.env.example"
        ]
    }
}
