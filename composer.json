{
    "name": "pacoorozco/ssham",
    "description": "SSH Access Manager Web Interface.",
    "keywords": [
        "ssh-key",
        "ssh",
        "ssh key manager",
        "keyring",
        "access management",
        "laravel"
    ],
    "homepage": "https://github.com/pacoorozco/ssham",
    "license": "GPL-3.0-or-later",
    "type": "project",
    "authors": [
        {
            "name": "Paco Orozco",
            "email": "paco@pacorozco.info",
            "homepage": "https://pacoorozco.info",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/pacoorozco/ssham/issues"
    },
    "require": {
        "php": "^8.0",
        "bensampo/laravel-enum": "^3.3",
        "fideloper/proxy": "^4.4.1",
        "fruitcake/laravel-cors": "^2.0.4",
        "guilhermegonzaga/presenter": "^1.0.5",
        "larapacks/setting": "^3.0",
        "laravel-json-api/laravel": "^1.0@beta",
        "laravel/framework": "^8.41",
        "laravel/sanctum": "^2.11",
        "laravel/ui": "^3.2",
        "laravelcollective/html": "^6.2.1",
        "phpseclib/phpseclib": "^2.0.31",
        "spatie/laravel-activitylog": "^4.0.0",
        "spatie/laravel-permission": "^4.2",
        "spatie/laravel-searchable": "^1.9.1",
        "yajra/laravel-datatables-oracle": "^9.18"
    },
    "require-dev": {
        "facade/ignition": "^2.8.3",
        "fakerphp/faker": "^1.14.1",
        "laravel-json-api/testing": "^1.0@beta",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^5.4",
        "phpunit/phpunit": "^9.5.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "test": [
            "@php artisan test"
        ],
        "test-coverage": [
            "@php artisan test --coverage-html test-coverage"
        ],
        "build": [
            "@composer archive --format=zip --dir=dist"
        ]
    },
    "archive": {
        "exclude": [
            "/utils",
            "/.*",
            "/docs",
            "!/.env.example"
        ]
    }
}
